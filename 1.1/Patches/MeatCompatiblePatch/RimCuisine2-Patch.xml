<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>[1.0, 1.1] RimCuisine 2 Core</li>
      <li>RimCuisine 2 Core</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/RecipeDef[defName = "CookThinPottage" or defName = "RC2_CookThickPottage"]/ingredients/li[1]/filter/categories</xpath>
          <value>
            <li>RF_RawSeafood</li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>